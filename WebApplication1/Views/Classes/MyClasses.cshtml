@model IEnumerable<WebApplication1.Models.Class>

@{
    ViewBag.Title = "My Classes";
}

<h2>My Classes</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Branch.Address)
        </th>
        <th>
            Trainer Name
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Time)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Branch.Address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Trainer.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Time)
            </td>
        </tr>
    }

</table>
