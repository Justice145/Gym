            @Html.DisplayFor(modelItem => item.FridayClose)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SaturdayOpen)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SaturdayClose)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
