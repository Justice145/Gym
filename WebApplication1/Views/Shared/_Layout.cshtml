<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @Styles.Render("~/Content/bootstrap.css")
    @Styles.Render("~/Content/style.css")
    @Styles.Render("~/Content/font-awesome.min.css")

    <title>Befit Sports Category Bootstrap Responsive Website Template | Contact Us :: W3layouts</title>
    <!-- Meta tag Keywords -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta charset="UTF-8" />
    <meta name="keywords" content="Befit Responsive web template, Bootstrap Web Templates, Flat Web Templates, Android Compatible web template, Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />
    <script>
        addEventListener("load", function () {
            setTimeout(hideURLbar, 0);
        }, false);

        function hideURLbar() {
            window.scrollTo(0, 1);
        }
    </script>
    <!-- //Meta tag Keywords -->
    <!-- Custom-Files -->
    <link rel="stylesheet" href="~/Content/bootstrap.css">
    <!-- Bootstrap-Core-CSS -->
    <link rel="stylesheet" href="~/Content/style.css" type="text/css" media="all" />
    <!-- Style-CSS -->
    <link href="~/Content/font-awesome.min.css" rel="stylesheet">
    <!-- Font-Awesome-Icons-CSS -->
    <!-- //Custom-Files -->
    <!-- Web-Fonts -->
    <link href="//fonts.googleapis.com/css?family=Nunito+Sans:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i&amp;subset=latin-ext,vietnamese"
          rel="stylesheet">
    <link href="//fonts.googleapis.com/css?family=Roboto+Condensed:300,300i,400,400i,700,700i&amp;subset=cyrillic,cyrillic-ext,greek,greek-ext,latin-ext,vietnamese"
          rel="stylesheet">
    <!-- //Web-Fonts -->
    <link rel="stylesheet" href="~/Content/ovveride.css">
</head>
<body>

    <header>
        <div class="container-fluid">
            <div class="header d-md-flex justify-content-between align-items-center py-3 px-xl-5 px-lg-3 px-2">
                <!-- logo -->
                <div id="logo">
                    <h1>@Html.ActionLink("Superfit", "Index", "Home")</h1>
                </div>
                <!-- //logo -->
                <!-- nav -->
                <div class="nav_w3ls">
                    <nav>
                        <label for="drop" class="toggle">Menu</label>
                        <input type="checkbox" id="drop" />
                        <ul class="menu">
                            <li>@Html.ActionLink("Home", "Index", "Home")</li>
                            @if (!User.Identity.IsAuthenticated)
                            {
                                <li>@Html.ActionLink("Register", "Register", "Account")</li>
                                <li>@Html.ActionLink("Login", "Login", "Account")</li>
                            }
                            else
                            {
                                <li>@Html.ActionLink("My Account", "Index", "Manage")</li>
                            }
                            <li class="mx-lg-4 mx-md-3 my-md-0 my-2">@Html.ActionLink("About", "About", "Home")</li>
                            <li><a href="gallery.html">Gallery</a></li>
                            <li class="mx-lg-4 mx-md-3 my-md-0 my-2">
                                <!-- First Tier Drop Down -->
                                <label for="drop-2" class="toggle toogle-2">
                                    Dropdown <span class="fa fa-angle-down" aria-hidden="true"></span>
                                </label>
                                <a href="#" class="active">Users Info <span class="fa fa-angle-down" aria-hidden="true"></span></a>
                                <input type="checkbox" id="drop-2" />
                                <ul>
                                    <li>@Html.ActionLink("Branches", "Index", "Branches")</li>
                                    @if (User.IsInRole("Admin"))
                                    {
                                        <li>@Html.ActionLink("Users", "Index", "Users")</li>
                                    }
                                    <li>@Html.ActionLink("Classes", "Index", "Classes")</li>
                                </ul>
                            </li>
                            <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                        </ul>
                    </nav>
                </div>
                <!-- //nav -->
            </div>
        </div>
    </header>

    <div>
        <div class="main-top" id="home">
            <!-- header -->
            <!-- //header -->
        </div>

        <head>
            <title>Befit Sports Category Bootstrap Responsive Website Template | Contact Us :: W3layouts</title>
            <!-- Meta tag Keywords -->
            <meta name="viewport" content="width=device-width, initial-scale=1">
            <meta charset="UTF-8" />
            <meta name="keywords" content="Befit Responsive web template, Bootstrap Web Templates, Flat Web Templates, Android Compatible web template, Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />
            <script>
                addEventListener("load", function () {
                    setTimeout(hideURLbar, 0);
                }, false);

                function hideURLbar() {
                    window.scrollTo(0, 1);
                }
            </script>
            <!-- //Meta tag Keywords -->
            <!-- Custom-Files -->
            <link rel="stylesheet" href="~/Content/bootstrap.css">
            <!-- Bootstrap-Core-CSS -->
            <link rel="stylesheet" href="~/Content/style.css" type="text/css" media="all" />
            <!-- Style-CSS -->
            <link href="~/Content/font-awesome.min.css" rel="stylesheet">
            <!-- Font-Awesome-Icons-CSS -->
            <!-- //Custom-Files -->
            <!-- Web-Fonts -->
            <link href="//fonts.googleapis.com/css?family=Nunito+Sans:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i&amp;subset=latin-ext,vietnamese"
                  rel="stylesheet">
            <link href="//fonts.googleapis.com/css?family=Roboto+Condensed:300,300i,400,400i,700,700i&amp;subset=cyrillic,cyrillic-ext,greek,greek-ext,latin-ext,vietnamese"
                  rel="stylesheet">
            <!-- //Web-Fonts -->
            <link rel="stylesheet" href="~/Content/ovveride.css">
        </head>

        <!-- main banner -->
        <div class="main-top" id="home">
            <!-- header -->
            <!-- //header -->
            <!-- banner -->
            <div class="banner_w3lspvt-2">

            </div>
            <!-- //banner -->
        </div>
        <!-- //main banner -->
        <!-- page details -->
        <div class="breadcrumb-agile">
            <ol class="breadcrumb">
                <li class="login" aria-current="page">@Html.Partial("_LoginPartial")</li>
                <li class="breadcrumb-item">
                    @Html.ActionLink("Home", "Index", "Home")
                </li>
                <li class="breadcrumb-item active" aria-current="page">User info</li>
                <li class="breadcrumb-item active" aria-current="page">Classes</li>
            </ol>
        </div>
        @RenderBody()
        <a href="#home" class="move-top text-center"></a>
        <footer>

            <div class="cpy-right text-center py-3 navbar-fixed-bottom">
                <p>
                    &copy; @DateTime.Now.Year - SuperFit. All rights reserved
                </p>
            </div>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/datetime")
    @RenderSection("scripts", required: false)
</body>


</html>
